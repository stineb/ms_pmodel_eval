	Eprint = {http://hol.sagepub.com/content/early/2015/06/01/0959683615588374.full.pdf+html},
	Journal = {The Holocene},
	Title = {{Amazonia and the Anthropocene: What was the spatial extent and intensity of human landscape modification in the Amazon Basin at the end of prehistory?}},
	Url = {http://hol.sagepub.com/content/early/2015/06/01/0959683615588374.abstract},
	Year = {2015},
