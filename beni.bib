	Isbn = {0004-5608},
	Issn = {0004-5608},
	Journal = {Annals of the Association of American Geographers},
	Number = {4},
	Pages = {755--771},
	Title = {{The Columbian Encounter and the Little Ice Age: abrupt land use change, fire, and greenhouse forcing}},
	Volume = {100},
	Year = {2010},
