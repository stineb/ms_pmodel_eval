	Isbn = {doi:10.1051/0004-6361/201015843},
	Issn = {0004-6361},
	Journal = {Astronomy and Astrophysics},
	Keywords = {activity,faculae,plages,solar-terrestrial relations,sun,sunspots,surface magnetism,uv radiation},
	Number = {A6},
