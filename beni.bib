	Journal = {Remote Sensing of Environment},
	Keywords = {GPP},
	Number = {3},
	Pages = {657 - 667},
	Title = {Combining remote sensing and ancillary data to monitor the gross productivity of water-limited forest ecosystems},
	Url = {http://www.sciencedirect.com/science/article/pii/S003442570800343X},
	Volume = {113},
	Year = {2009},
	Bdsk-Url-1 = {http://www.sciencedirect.com/science/article/pii/S003442570800343X},
