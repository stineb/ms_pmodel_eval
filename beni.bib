	Issn = {0028-0836},
	Journal = {Nature},
	Number = {7263},
	Pages = {507--510},
	Pmid = {19779448},
	Publisher = {Nature Publishing Group},
	Title = {{Stable isotope constraints on Holocene carbon cycle changes from an Antarctic ice core.}},
	Url = {http://dx.doi.org/10.1038/nature08393},
	Volume = {461},
	Year = {2009},
